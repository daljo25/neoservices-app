<?xml version='1.0' encoding='utf-8'?>
<widget id="com.daljo25.neoservices" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Neoservices</name>
    <description>
        App oficial de Neoservices.com.ve y Neoservices.cl
    </description>
    <author email="daljomar.morillo@gmail.com" href="https://daljomarmorillo.com.ve">
        Daljo25
    </author>
    <content src="index.html" />
    <splash src="splash.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="whatsapp:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="25" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="50000" />
</widget>
